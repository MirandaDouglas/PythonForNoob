print("Calculadora de tempo")
print("Qual o primeiro horario?")

hr_inicio = float(input("Horas: "))
min_inicio = float(input("Minutos: "))

print("Qual o segundo horario?")

hr_fim = float(input("Horas: "))
min_fim = float(input("Minutos: "))

hr_auxiliar = (hr_inicio-hr_fim)
min_auxiliar = (min_inicio-min_fim)

print(hr_auxiliar + min_auxiliar)
