print("axÂ²+bx+c")

a = float(input("Valor de A"))
b = float(input("Valor de B"))
c = float(input("Valor de C"))

delta = (b*b)-(4*a*c)
if delta < 0:
  print('Numeros Complexos ')
   
elif delta > 0:
  raizQ = delta ** (1/2)
  bhaskaraPositiva = ((-b) + raizQ)
  bhaskaraNegativa = ((-b) - raizQ)
  resultadoPositivo = bhaskaraPositiva/2*a
  resultadoNegativo = bhaskaraNegativa/2*a
  print("x1", resultadoNegativo)
  print("x2", resultadoPositivo)
