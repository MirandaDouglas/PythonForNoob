print("axÂ²+bx+c")

a = float(input("Valor de A: "))
b = float(input("Valor de B: "))
c = float(input("Valor de C: "))


delta = (b*b)-(4*a*c)
if delta < 0:
  print('Numeros Complexos ')
   
elif delta > 0:
  raizQ = delta ** (1/2)
  bhaskaraPositiva = ((-b) + raizQ)
  bhaskaraNegativa = ((-b) - raizQ)
  x1 = bhaskaraPositiva/2*a
  x2 = bhaskaraNegativa/2*a

#arrendondar valores em 2 casas decimais

  x1 = round(x1,2)
  x2 = round(x2,2)
  print("x1", x1)
  print("x2", x2)
